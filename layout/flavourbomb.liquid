    {{ content_for_header }}
  
    {{ 'bootstrap.css' | asset_url | stylesheet_tag:preload: true }}
  {{ 'flavour-bombs.css' | asset_url | stylesheet_tag:preload: true}}

  <link href="https://cdnjs.cloudflare.com/ajax/libs/lity/2.4.1/lity.min.css" rel="stylesheet">
  
    {% render 'styles-scripts' %}
    <script>
    document.addEventListener('DOMContentLoaded', function () {
      // Select all anchor tags
      var links = document.querySelectorAll('a');
  
      links.forEach(function (link) {
        // Check if the link is external
        if (link.hostname !== window.location.hostname) {
          // Set the target to open in a new tab
          link.setAttribute('target', '_blank');
          link.setAttribute('rel', 'noopener noreferrer'); // Security best practice
        }
      });
    });
  </script>
  
  <link rel="stylesheet" href="https://use.typekit.net/kfo6dzh.css">
  <link rel="preload" href="https://cdn.shopify.com/s/files/1/0583/8415/4690/files/Royal-Rumble.woff2" as="font" type="font/woff2" crossorigin>
  <link rel="preload" href="https://cdn.shopify.com/s/files/1/0583/8415/4690/files/Royal-Rumble.woff" as="font" type="font/woff" crossorigin>
  <style>
  @font-face {
      font-family: 'acumin-pro-condensed';
      font-weight: 300;
      font-style: normal;
      font-display: swap;
  }
  @font-face {
    font-family: 'acumin-pro-extra-condensed';
    font-weight: 300;
    font-style: normal;
    font-display: swap;
  }
  @font-face {
      font-family: 'royal-rumble';
      src: url('https://cdn.shopify.com/s/files/1/0583/8415/4690/files/Royal-Rumble.woff2') format('woff2'),
          url('https://cdn.shopify.com/s/files/1/0583/8415/4690/files/Royal-Rumble.woff') format('woff');
      font-weight: 400;
      font-style: normal;
      font-display: swap;
  }
  </style>
  
     {% render 'lazyload_img' %} 
    
  {% render 'sb-cls-improve' %} <script data-custom-wpsob="1">function wpsobIsInViewport(el){const rect=el.getBoundingClientRect();return rect.top>=0&&rect.left>=0&&rect.bottom<=(window.innerHeight||document.documentElement.clientHeight)&&rect.right<=(window.innerWidth||document.documentElement.clientWidth)}function wpsobRemoveDims(img){if(img.hasAttribute("data-wpsob-width"))img.removeAttribute("width");if(img.hasAttribute("data-wpsob-height"))img.removeAttribute("height")}function wpsobCheckVisibleImages(){const imgs=document.querySelectorAll("img");for(let i=0;i<imgs.length;i++)if(wpsobIsInViewport(imgs[i]))wpsobRemoveDims(imgs[i])}function wpsobRemoveDimsAll(){const imgs=document.querySelectorAll("img");for(let i=0;i<imgs.length;i++)wpsobRemoveDims(imgs[i])}window.onload=wpsobCheckVisibleImages;window.addEventListener("scroll",wpsobRemoveDimsAll);</script></head>
  
  <body class="{{ request.page_type | prepend: 'page-type-' }} {{ template }}">
    <a class="visually-hidden-focusable" href="#main">
      {{ 'general.accessibility.skip_content' | t }}
    </a>
  
